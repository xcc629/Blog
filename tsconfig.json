{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "baseUrl": ".",
    "paths": {
      "@src/*": ["src/*"],
      "@style/*": ["src/blog_component/_style/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "types/baseProps.tsz"],
  "exclude": ["node_modules"]
}
